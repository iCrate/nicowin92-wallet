  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^12.12.0",
    "@types/text-encoding": "0.0.35",
    "jest": "^24.1.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "typescript": "3.4.2"
  },
  "jest": {
    "preset": "ts-jest",
