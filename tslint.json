{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-standard"],
  "linterOptions": {
    "exclude": ["**/node_modules/**", "**/lib/**"]
  },
  "rules": {
    "no-submodule-imports": false,
    "semicolon": true,
    "no-implicit-dependencies": [true, "dev"],
    "jsx-no-multiline-js": false,
    "object-literal-sort-keys": false
  }
}
